{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@5.0.1",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "library": "tsiclient@1.4.8",
      "destination": "wwwroot/lib/tsiclient/"
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.15.1",
      "destination": "wwwroot/css/font-awesome/"
    }
  ]
}